	cp $(SoftShadows_release_DEPDIR).d $(addprefix $(DEPSDIR)/SoftShadows/release/, $(subst ./, , $(subst ../, , $(filter %$(strip $(subst .c.o,.c, $(subst $(SoftShadows_release_objsdir),, $@))), $(SoftShadows_cfiles))))).P; \
	  sed -e 's/#.*//' -e 's/^[^:]*: *//' -e 's/ *\\$$//' \
		-e '/^$$/ d' -e 's/$$/ :/' < $(SoftShadows_release_DEPDIR).d >> $(addprefix $(DEPSDIR)/SoftShadows/release/, $(subst ./, , $(subst ../, , $(filter %$(strip $(subst .c.o,.c, $(subst $(SoftShadows_release_objsdir),, $@))), $(SoftShadows_cfiles))))).P; \
	  rm -f $(SoftShadows_release_DEPDIR).d

clean_SoftShadows:  clean_SoftShadows_debug clean_SoftShadows_release
	rm -rf $(DEPSDIR)

